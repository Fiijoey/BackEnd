<div class="login-container">
  <h1>
    <%= title %>
  </h1>
  <%- messages() %>
    <form method="POST" action="/account/login">
      <div>
        <input type="email" name="account_email" placeholder="Email Address" required>
      </div>
      <div>
        <input type="password" name="account_password" placeholder="Password" required>
      </div>
      <div>
        <button type="submit">Login</button>
      </div>
    </form>
    <p>Don't have an account? <a href="/account/register">Register Here</a></p>
</div>